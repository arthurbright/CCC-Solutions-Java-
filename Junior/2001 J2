import java.io.*;
import java.util.*;


public class Solution {

	
	
	public static void main(String[] args)
	{
		Scanner sc = new Scanner(System.in);
		int x = sc.nextInt();
		int mod = sc.nextInt();
		int ans = -1;
		
		for(int i = 0; i <= mod; i ++)
		{
			if(i * x % mod == 1)
			{
				ans = i;
			}
		}
		if(ans == -1)
		{
			System.out.println("No such integer exists.");
		}
		else
		{
			System.out.println(ans);
		}
		
	}
	
	

	
}


