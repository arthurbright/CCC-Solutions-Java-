#include <iostream>
#include <cmath>
#include <map>
#include <vector>
#include <algorithm>
#include <queue>
#include <stack>

using namespace std;

int main()
{
   int n;
   cin >> n;
   int y, m, d;


   for(int i = 0; i < n; i ++)
   {
       cin >> y;
       cin >> m;
       cin >> d;
       if(y <= 1988)
       {
           cout << "Yes" << endl;
       }
       else if(y == 1989)
       {
           if(m < 2)
           {
               cout << "Yes" << endl;
           }
           else if(m == 2)
           {
               if(d <= 27)
               {
                   cout << "Yes" << endl;
               }
               else{
                cout << "No" << endl;
               }
           }
           else{
            cout << "No" << endl;
           }
       }
       else
       {
           cout << "No" << endl;
       }
   }


}
